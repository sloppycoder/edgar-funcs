            "args": [
                "--embedding-model=text-embedding-3-small",
                "--embedding-dimension=1536",
                "--extraction-model=gemini-2.0-flash",
                "fundmgr",
                // "tmp/t.csv"
                //"0001224568-24-000005",
                "0.1",
